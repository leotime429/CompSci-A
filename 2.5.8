import java.util.Scanner;

public class CalculatorTester
{
    public static void main(String[] args)
    {
        Scanner input = new Scanner(System.in);
        Calculator calc = new Calculator();
        
        double x, y;
        
        System.out.println("Enter two doubles");
        x = input.nextDouble();
        y = input.nextDouble();
        
        calc.add(x,y);
        calc.subtract(x,y);
        calc.multiply(x,y);
        calc.divide(x,y);
        
    }
}
